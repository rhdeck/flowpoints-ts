{
  "name": "@raydeck/flowpoints",
  "private": false,
  "version": "1.0.0",
  "description": "",
  "main": "lib/index.js",
  "bin": "lib/bin.js",
  "scripts": {
    "setup": "./init-template.js",
    "pre-commit": "(git-branch-is --not main || (set -u && echo $ALLOWMAIN)) && yarn test",
    "test": "tsc --noEmit",
    "build": "tsc && chmod +x lib/bin.js && yarn pack -f package.tgz",
    "docs:bin": "mkdir docstemp ; (commanderdoc markdown lib/bin.js -o docstemp/__cli.md --cli-name \"npx `jq -r .name package.json`\") ; ",
    "docs:lib": "typedoc --theme markdown --readme none --mode file --excludeNotExported --includeVersion --out docstemp  && rm README.md ; mv docstemp/README.md docstemp/_library.md",
    "docs:compile": "mkdir docstemp ; cp TOP.md docstemp/README.md ; concat-md docstemp > README.md && rm -rf docstemp",
    "docs": "rm -rf docstemp && yarn docs:lib && yarn docs:bin && yarn docs:compile",
    "prepublishOnly": "yarn build && yarn docs && git commit README.md -m \"docs: updating docs\" && git push",
    "watch": "livelink watch",
    "prepare": "install-peers"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rhdeck/flowpoints-ts.git"
  },
  "author": "Ray Deck",
  "license": "MIT",
  "bugs": {
    "url": "https:/github.com/rhdeck/flowpoints-ts/issues"
  },
  "homepage": "https://github.com/rhdeck/flowpoints-ts#README",
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@raydeck/livelink": "^1.5.2",
    "@types/inquirer": "^7.3.1",
    "@types/node": "^14.14.17",
    "@types/react": "^17.0.19",
    "@types/validate-npm-package-name": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "commanderdoc": "^0.1.11",
    "concat-md": "^0.3.5",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.24.0",
    "git-branch-is": "^4.0.0",
    "husky": "^4.3.6",
    "install-peers-cli": "^2.2.0",
    "typedoc": "^0.21.6",
    "typedoc-plugin-markdown": "^3.10.4",
    "typescript": "^4.4.2",
    "validate-npm-package-name": "^3.0.0"
  },
  "files": [
    "src",
    "lib"
  ],
  "dependencies": {
    "commander": "^6.2.1",
    "inquirer": "^7.3.3"
  },
  "liveLink": {
    "ignoreMasks": [
      "package.tgz",
      "lib/**",
      "README.md",
      "docs/**",
      "docstemp/**"
    ]
  },
  "peerDependencies": {
    "react": "^17.0.2"
  }
}
